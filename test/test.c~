void countSort(int *mas, int n, int exp) {
    
    int i, count[10] = {0};
    int output[n];
    
    for (i = 0; i < n; i++) {
        count[ (mas[i] / exp) % 10 ]++;
    }
 
    for (i = 1; i < 10; i++) {
        count[i] += count[i - 1];
    }
    
    for (i = n - 1; i >= 0; i--) {
        output[count[ (mas[i] / exp) % 10 ] - 1] = mas[i];
        count[ (mas[i] / exp) % 10 ]--;
    } 
 

    for (i = 0; i < n; i++) {
        mas[i] = output[i];
    }

}

void radixsort(int *mas, int n) {

    int max, exp;
    max = getMax(mas, n);
    
    for(exp = 1; max / exp > 0; exp*=10) {
        countSort(mas, n, exp);   	
    }	
}
 
void print(int *mas, int n) {

    int i;

    for (i = 0; i < n; i++) {
        printf("%d ", mas[i]);
    }

    printf("\n");
} */
 
int main() {

    int n, i;
    scanf("%d", &n);
    int mas[n];
    double t;

    t = wtime();

    for(i = 0; i < n; i++) {
         // mas[i] = rand ()%100;
        mas[i] = getrand(0,100000);       	
    }

    radixsort(mas, n);    
    //print(mas, n);
    
    t = wtime() - t;

    printf("Elapsed time: %.6f sec.\n", t);  
  
    return 0;
}
